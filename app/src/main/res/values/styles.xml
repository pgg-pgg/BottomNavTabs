<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!--底部Tabbar菜单栏的自定义属性-->
    <declare-styleable name="TabBar_Attr">
        <attr name="icon" format="reference"/>
        <attr name="name" format="string"/>
        <attr name="imgWidth" format="dimension"/>
        <attr name="imgHeight" format="dimension"/>
        <attr name="isShowDot" format="boolean"/>
        <attr name="isSelected" format="boolean"/>
        <attr name="tvColors" format="reference"/>
    </declare-styleable>

</resources>
